.container{
  width:100%;
  height:100%;
  background:#fff;
}
.hotelInfo{
  width:100%;
  padding-left: 20rpx; 
  box-sizing: border-box;  
}
.userInfo{
  width:100%;
}
.weui-cell{
  border-bottom:1rpx solid #f2f2f2;
}
.height{height:70rpx;line-height: 70rpx;}
.weui-cell.weui-cell_input:last-of-type{
  border-bottom:0px;
} 
.weui-check__label.weui-cell:last-of-type{
  border-bottom:none;
}
.weui-check__label.weui-cell{
  padding-left:0rpx;
}
 .weui-cell:before,.weui-cells:before{
  border-top:none;
} 
.weui-cell:after,.weui-cells:after{
  border:none;
}
.weui-select{
  border-right:none;
  border-top:none;
}
.weui-cell_select{
  border-top:1rpx solid #f2f2f2;
}
.weui-check_label{
  border-bottom:none;
  padding-bottom:30rpx;
}
.bd{
  flex:3;
}
.ft{
  flex:7;
  text-align: left;
} 
.weui-label{
  width:195rpx;
}
.tips{
  width:100%;
  line-height: 30rpx;
  height:250rpx;
  margin-top:20rpx; 
  font-weight:500;
}
.tipTxt{
  padding-left: 20rpx;
  box-sizing: border-box;
}
.tipContent{
  width:100%;
  font-size:24rpx;
  color:#bbb;
  line-height: 24rpx;
  margin-top: 20rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.bigBox{
  margin-left:20rpx; 
  background: #fff;
}
.maxBox{
  width:100%;
  background: #fff;
  border-bottom:1rpx solid #f2f2f2;
  border-top:1rpx solid #f2f2f2;
}
.holder{
  font-size: 30rpx;
  color:#ddd;
}

