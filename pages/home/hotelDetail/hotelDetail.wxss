.banner_img {
  width: 100%;
  height: 100%;
}
.title {
  background: #fff;
}
.name {
  font-weight: 600;
  font-size: 30rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f2f2f2;
}
.bigBox {
  background: #fff;
  margin-top: 20rpx;
}
.serviceTitle {
  font-weight: 600;
  font-size: 26rpx;
  line-height: 80rpx;  
  padding-left: 20rpx;
}
.serviceList {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  border-bottom: 1rpx solid #f2f2f2; 
}
.service {
  width: 80rpx;
  height: 80rpx;
  line-height: 20rpx;
  margin: 20rpx;
  text-align: center;
}
.icon_txt {
  font-size: 20rpx;
  color: #bbb;
  line-height: 60rpx
}
.content {
  width: 95%;
  margin: 0 auto;
  font-size: 24rpx;
  line-height: 50rpx; 
  color: #aaa; 
  padding-bottom: 30rpx; 
}
.weui-cells{
  position: static
}
.weui-cell{
  padding: 20rpx 20rpx;
}
.weui-cells:after{
  border: none
}